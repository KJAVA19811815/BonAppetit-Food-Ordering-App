{
    "env": {
        "node": true,
        "es6": true,
        "jquery": true,
        "browser": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
        "sourceType": "module"
    },
    "rules": {
        "indent": [
            "error",
            2
        ],
        "linebreak-style": [
            "warn",
            "unix"
        ],
        "quotes": [
            "warn",
            "single"
        ],
        "semi": ["error", "always"],
        "no-console": 1,
        "no-multi-spaces": "warn"
    }
}
